{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "TS All Debugger",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/output/run.js",
            "stopOnEntry": false,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/output/*.js"
            ]
        }
    ]
}